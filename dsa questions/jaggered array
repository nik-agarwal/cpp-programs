// arrays of pointer
// ex-   1 3 4
//       4 
//       3 5 5 6
//       4 5
         
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int rows;
    cin>>rows;
    int *col = new int[rows];
    int **arr = new int*[rows];
    for(int i=0;i<rows;++i){
        cin>>col[i];
        arr[i]=new int[col[i]];
    }
    for(int i=0;i<rows;++i){
        for(int j=0;j<col[i];++j){
            cin>>arr[i][j];
        }
    }
     for(int i=0;i<rows;++i){
        for(int j=0;j<col[i];++j){
            cout<<arr[i][j];
        }
        cout<<endl;
    }
    return 0;
}
